@model DbObjektyModel

@{
    ViewBag.Title = "Databázové objekty";
}

<div class="container-fluid mt-3 overflow-auto">
    <div class="d-flex gap-5">
        <div class="d-flex flex-column">
            <h3>Tabulky</h3>
            <p>
                @foreach (var item in Model.Tabulky)
                {
                    @item
                    <br>
                }
            </p>
        </div>
        <div class="d-flex flex-column">
            <h3>Pohledy</h3>
            <p>
                @foreach (var item in Model.Pohledy)
                {
                    @item
                    <br>
                }
            </p>
        </div>
        <div class="d-flex flex-column">
            <h3>Indexy</h3>
            <p>
                @foreach (var item in Model.Indexy)
                {
                    @item
                    <br>
                }
            </p>
        </div>
        <div class="d-flex flex-column">
            <h3>Package</h3>
            <p>
                @foreach (var item in Model.Package)
                {
                    @item
                    <br>
                }
            </p>
        </div>
        <div class="d-flex flex-column">
            <h3>Procedury</h3>
            <p>
                @foreach (var item in Model.Procedury)
                {
                    @item
                    <br>
                }
            </p>
        </div>
        <div class="d-flex flex-column">
            <h3>Funkce</h3>
            <p>
                @foreach (var item in Model.Funkce)
                {
                    @item
                    <br>
                }
            </p>
        </div>
        <div class="d-flex flex-column">
            <h3>Triggery</h3>
            <p>
                @foreach (var item in Model.Triggery)
                {
                    @item
                    <br>
                }
            </p>
        </div>
    </div>
</div>